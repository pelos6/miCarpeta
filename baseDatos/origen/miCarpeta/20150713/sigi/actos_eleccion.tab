create table ACTOS_ELECCION
(
  F_CONV_AE             DATE not null,
  DESCRIP_AE            VARCHAR2(50) not null,
  C_TIPO_AE             VARCHAR2(1) not null,
  F_FIN_SOLIC           DATE not null,
  C_FASE_AE             VARCHAR2(1) not null,
  PA_ULT_FOFER_VAC_CCAO DATE not null,
  TITULO1               VARCHAR2(200) not null,
  TITULO2               VARCHAR2(200) not null,
  TITULO3               VARCHAR2(200) not null,
  C_USU_ULTMOD          VARCHAR2(8) not null,
  F_ULTMOD              DATE not null,
  L_PET_MAS             VARCHAR2(1) default 'N' not null
)
;
comment on table ACTOS_ELECCION
  is ' Actos de Elección existentes. ';
comment on column ACTOS_ELECCION.F_CONV_AE
  is ' Fecha de la resolución de la convocatoria del AE.  ';
comment on column ACTOS_ELECCION.DESCRIP_AE
  is ' Descripción AE.  ';
comment on column ACTOS_ELECCION.C_TIPO_AE
  is ' Código del tipo de AE.  ';
comment on column ACTOS_ELECCION.F_FIN_SOLIC
  is ' Fecha final de admisión de solicitudes para el AE.  ';
comment on column ACTOS_ELECCION.C_FASE_AE
  is ' Fase en la que se encuentra el Acto de Elección. ';
comment on column ACTOS_ELECCION.C_USU_ULTMOD
  is ' Usuario que ha realizado la última modificación del registro. ';
comment on column ACTOS_ELECCION.F_ULTMOD
  is ' Fecha en que se ha realizado la última modificación del registro. ';
comment on column ACTOS_ELECCION.L_PET_MAS
  is 'Indica si se visualiza o no la opción de peticiones masivas por provincia/especialidad. Es ''S'' para los de tipo I y tipo F del colectivo 7 pero se informará manualmente';
alter table ACTOS_ELECCION
  add constraint PK_ACTOS_ELECCION primary key (F_CONV_AE);

