create table PETICPRO_AE
(
  F_CONV_AE    DATE not null,
  TIPO_ID      VARCHAR2(1) not null,
  DNI          VARCHAR2(15) not null,
  C_SOL        VARCHAR2(4) not null,
  AÑO_INI_CUR  VARCHAR2(4) not null,
  C_PROV       VARCHAR2(2) not null,
  N_ORDEN_PROV NUMBER(1) not null
)
;
comment on table PETICPRO_AE
  is 'Peticiones de vacantes por provincia.';
comment on column PETICPRO_AE.F_CONV_AE
  is 'Acto de Elección al que corresponde la petición. ';
comment on column PETICPRO_AE.TIPO_ID
  is ' Tipo de Identificador del candidato preseleccionado. ';
comment on column PETICPRO_AE.DNI
  is ' Dni del candidato preseleccionado. ';
comment on column PETICPRO_AE.C_SOL
  is 'Código de solicitud de ese candidato para este Acto de Elección.';
comment on column PETICPRO_AE.AÑO_INI_CUR
  is ' Año de inicio de curso academico al que corresponde la Vacante. ';
comment on column PETICPRO_AE.C_PROV
  is ' Código de la provincia por la que se piden todas las vacantes obligatorias. ';
comment on column PETICPRO_AE.N_ORDEN_PROV
  is ' Número de preferencia de la provincia. ';
alter table PETICPRO_AE
  add constraint PK_PETICPRO_AE primary key (F_CONV_AE, TIPO_ID, DNI, AÑO_INI_CUR, C_PROV, C_SOL);
alter table PETICPRO_AE
  add constraint FK_PETPAE_SOAEI foreign key (F_CONV_AE, TIPO_ID, DNI, C_SOL)
  references SOLICITUDES_AE_INTERNET (F_CONV_AE, TIPO_ID, DNI, C_SOL);

