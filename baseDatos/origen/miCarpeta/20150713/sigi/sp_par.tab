create table SP_PAR
(
  TIPO_ID         VARCHAR2(1) not null,
  DNI             VARCHAR2(15) not null,
  NOMBRE          VARCHAR2(14),
  APELLIDO1       VARCHAR2(22),
  APELLIDO2       VARCHAR2(22),
  EST_PER         VARCHAR2(10),
  D_CAUSA_EST_PER VARCHAR2(177),
  F_INI_PER       DATE
)
;
alter table SP_PAR
  add constraint SP_PAR_PK primary key (TIPO_ID, DNI);

