create table RESERVAS_AE_INTERNET
(
  F_CONV_AE    DATE not null,
  TIPO_ID      VARCHAR2(1) not null,
  DNI          VARCHAR2(15) not null,
  C_SOL        VARCHAR2(4) not null,
  C_ESTADO_RES VARCHAR2(1) not null
)
;
comment on table RESERVAS_AE_INTERNET
  is 'Reservas ejercidas en solicitudes de AE de Inicio de Curso.';
comment on column RESERVAS_AE_INTERNET.F_CONV_AE
  is ' Acto de Elección al que corresponde la reserva.  ';
comment on column RESERVAS_AE_INTERNET.TIPO_ID
  is ' Tipo de identificador del candidato preseleccionado. ';
comment on column RESERVAS_AE_INTERNET.DNI
  is ' Dni del candidato preseleccionado. ';
comment on column RESERVAS_AE_INTERNET.C_SOL
  is 'Código de solicitud de ese candidato para este Acto de Elección.';
comment on column RESERVAS_AE_INTERNET.C_ESTADO_RES
  is ' Código de estado de la reserva. ';
alter table RESERVAS_AE_INTERNET
  add constraint PK_RESERVAS_AE_INT primary key (F_CONV_AE, TIPO_ID, DNI, C_SOL);
alter table RESERVAS_AE_INTERNET
  add constraint FK_REAEI_SOAEI foreign key (F_CONV_AE, TIPO_ID, DNI, C_SOL)
  references SOLICITUDES_AE_INTERNET (F_CONV_AE, TIPO_ID, DNI, C_SOL);

