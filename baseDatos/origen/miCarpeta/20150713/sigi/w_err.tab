create table W_ERR
(
  NUM_ERR INTEGER not null,
  USE     VARCHAR2(30),
  TIPO_ID VARCHAR2(1),
  DNI     VARCHAR2(15),
  RET     VARCHAR2(4000),
  ERR_COD INTEGER,
  ERR_MEN VARCHAR2(4000),
  FEC_CRE DATE
)
;
comment on table W_ERR
  is 'Tabla de errores producidos en SIGIP';
comment on column W_ERR.NUM_ERR
  is 'Clave primaria generada por trigger';
comment on column W_ERR.USE
  is 'Usuario ';
comment on column W_ERR.RET
  is 'retorno con la traza del error ordenada y el código de error de aplicación';
comment on column W_ERR.ERR_COD
  is 'Código de error';
comment on column W_ERR.ERR_MEN
  is 'Mensaje de error';
comment on column W_ERR.FEC_CRE
  is 'Fecha de creación del registro';
alter table W_ERR
  add constraint PK_ERR primary key (NUM_ERR);

