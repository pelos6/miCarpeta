create table VACANTES_AE
(
  F_CONV_AE   DATE not null,
  AÑO_INI_CUR VARCHAR2(4) not null,
  N_VACANTE   NUMBER(4) not null,
  C_CUERPO    VARCHAR2(4) not null,
  C_ESPEC     VARCHAR2(3) not null,
  C_MUN_VAC   VARCHAR2(9) not null,
  C_TDURAC_V  VARCHAR2(1) not null
)
;
comment on table VACANTES_AE
  is ' Vacantes ofertadas en Acto de Elección. ';
comment on column VACANTES_AE.F_CONV_AE
  is ' Fecha del Acto de Elección.  ';
comment on column VACANTES_AE.AÑO_INI_CUR
  is ' Año de inicio del curso academico al que corresponde la Vacante.  ';
comment on column VACANTES_AE.N_VACANTE
  is ' Número identificativo de la vacante dentro del curso academico. ';
comment on column VACANTES_AE.C_CUERPO
  is ' Código del cuerpo al que corresponde la vacante.  ';
comment on column VACANTES_AE.C_ESPEC
  is ' Código de la especialidad a la que corresponde la vacante.  ';
comment on column VACANTES_AE.C_MUN_VAC
  is 'Municipio / Localidad (a partir de 2012) en el que ha estado ocupando plaza el participante. ';
comment on column VACANTES_AE.C_TDURAC_V
  is ' Código del tipo de Duración de la vacante.';
alter table VACANTES_AE
  add constraint PK_VACANTES_AE primary key (F_CONV_AE, AÑO_INI_CUR, N_VACANTE);
alter table VACANTES_AE
  add constraint FK_VACAE_ACTEL foreign key (F_CONV_AE)
  references ACTOS_ELECCION (F_CONV_AE);

