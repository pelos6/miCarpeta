  is 'Descripción provincia de refenencia';
alter table SP_LIS
  add constraint PK_SIT_PAR primary key (TIPO_ID, DNI, COD_CUE, COD_ESP, TIP_LIS, FEC_ORI_BLO, COD_BLO, COD_EST);
alter table SP_LIS
