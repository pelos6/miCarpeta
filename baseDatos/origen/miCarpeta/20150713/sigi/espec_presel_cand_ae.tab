create table ESPEC_PRESEL_CAND_AE
(
  F_CONV_AE    DATE not null,
  TIPO_ID      VARCHAR2(1) not null,
  DNI          VARCHAR2(15) not null,
  C_CUERPO     VARCHAR2(4) not null,
  C_ESPEC      VARCHAR2(3) not null,
  C_PROV1_SUSP VARCHAR2(2),
  C_PROV2_SUSP VARCHAR2(2),
  L_LISTA_SUSP VARCHAR2(1) not null
)
;
comment on table ESPEC_PRESEL_CAND_AE
  is ' Especialidades por las que se ha preseleccionado a los candidatos preseleccionados. ';
comment on column ESPEC_PRESEL_CAND_AE.F_CONV_AE
  is 'Acto de Elección al que corresponde la preselección. ';
comment on column ESPEC_PRESEL_CAND_AE.TIPO_ID
  is ' Tipo de Identificador del candidato preseleccionado. ';
comment on column ESPEC_PRESEL_CAND_AE.DNI
  is ' Dni del candidato preseleccionado. ';
comment on column ESPEC_PRESEL_CAND_AE.C_CUERPO
  is ' Código del cuerpo de la lista por la que se le preselecciona. ';
comment on column ESPEC_PRESEL_CAND_AE.C_ESPEC
  is ' Código de la especialidad de la lista por la que se le preselecciona. ';
comment on column ESPEC_PRESEL_CAND_AE.C_PROV1_SUSP
  is ' Código de la primera provincia en la que está suspendido dentro de la lista. ';
comment on column ESPEC_PRESEL_CAND_AE.C_PROV2_SUSP
  is ' Código de la segunda provincia en la que está suspendido dentro de la lista. ';
comment on column ESPEC_PRESEL_CAND_AE.L_LISTA_SUSP
  is 'Sí está, o no, suspendido en la lista . ';
alter table ESPEC_PRESEL_CAND_AE
  add constraint PK_ESPEC_PRESEL_CAND_AE primary key (F_CONV_AE, TIPO_ID, DNI, C_CUERPO, C_ESPEC);
alter table ESPEC_PRESEL_CAND_AE
  add constraint FK_EPCAE_CAPAE foreign key (F_CONV_AE, TIPO_ID, DNI)
  references CAND_PRESEL_AE (F_CONV_AE, TIPO_ID, DNI);
alter table ESPEC_PRESEL_CAND_AE
  add constraint CK_EPCAE_L_LISTA_SUSP
  check (L_LISTA_SUSP IN ('S','N'));

