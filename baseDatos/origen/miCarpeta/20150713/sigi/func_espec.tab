create table FUNC_ESPEC
(
  C_FUNC VARCHAR2(3) not null,
  D_FUNC VARCHAR2(80) not null
)
;
comment on table FUNC_ESPEC
  is ' Relación de Especialidades.';
comment on column FUNC_ESPEC.C_FUNC
  is ' Código de la Especialidad. ';
comment on column FUNC_ESPEC.D_FUNC
  is ' Descripción de la Especialidad. ';
alter table FUNC_ESPEC
  add constraint PK_FUNC_ESPEC primary key (C_FUNC);

