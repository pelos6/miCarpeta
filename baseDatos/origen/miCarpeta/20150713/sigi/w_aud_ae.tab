create table W_AUD_AE
(
  TIPO_ID   VARCHAR2(1),
  DNI       VARCHAR2(15),
  COD_SES   VARCHAR2(100),
  IP        VARCHAR2(40),
  VAL_1     VARCHAR2(4000),
  VAL_2     VARCHAR2(4000),
  NUM_REG   NUMBER not null,
  FECHA_INS DATE
)
;
comment on table W_AUD_AE
  is 'Tabla para la auditoria de los Actos de eleccion por web';
comment on column W_AUD_AE.TIPO_ID
  is 'Tido de identificador del participante';
comment on column W_AUD_AE.DNI
  is 'Dni del del participante';
comment on column W_AUD_AE.NUM_REG
  is 'número del registro';
alter table W_AUD_AE
  add constraint PK_W_AUD_AE primary key (NUM_REG);

