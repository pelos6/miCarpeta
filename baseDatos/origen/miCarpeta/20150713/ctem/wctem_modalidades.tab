create table WCTEM_MODALIDADES
(
  CMODA VARCHAR2(1) not null,
  DMODA VARCHAR2(120)
)
;
comment on table WCTEM_MODALIDADES
  is 'Modalidades de concurso';
comment on column WCTEM_MODALIDADES.CMODA
  is 'Código de Modalidad';
comment on column WCTEM_MODALIDADES.DMODA
  is 'Denominación de la modalidad';
alter table WCTEM_MODALIDADES
  add constraint PK_WCTEM_MODALIDADES primary key (CMODA);

