create table WCTEM_CUERPOSCONV
(
  CUERPO      VARCHAR2(4) not null,
  DCUERPO     VARCHAR2(60),
  CUERPO_PART VARCHAR2(4)
)
;
comment on table WCTEM_CUERPOSCONV
  is 'Tabla que contiene los códigos de los cuerpos admitidos en el concurso de traslados de E.E.M.M.';
comment on column WCTEM_CUERPOSCONV.CUERPO
  is 'Cuerpos convocados al concurso por los que pueden participar.';
comment on column WCTEM_CUERPOSCONV.CUERPO_PART
  is 'Cuerpo por el que participará internamente en el concurso (solo habrá diferencias en los catedráticos).';
alter table WCTEM_CUERPOSCONV
  add constraint PK_CUERPOSCONV primary key (CUERPO);

