create table WCTEM_ESPEC
(
  CESPEC VARCHAR2(3),
  CCODCU VARCHAR2(4),
  DESPEC VARCHAR2(65),
  ORDEN  NUMBER(3) not null
)
;
comment on table WCTEM_ESPEC
  is 'Tabla de especialidades convocadas en el Concurso de Traslados de EE.MM.';
comment on column WCTEM_ESPEC.CESPEC
  is 'Código de especialidad';
comment on column WCTEM_ESPEC.CCODCU
  is 'Cuerpo a que pertenece la especialidad';
comment on column WCTEM_ESPEC.DESPEC
  is 'Denominación de la especialidad';
alter table WCTEM_ESPEC
  add constraint PK_WCTEM_ESPEC primary key (CESPEC, CCODCU);
alter table WCTEM_ESPEC
  add constraint NN_CCODCU
  check ("CCODCU" IS NOT NULL);
alter table WCTEM_ESPEC
  add constraint NN_CESPEC
  check ("CESPEC" IS NOT NULL);
alter table WCTEM_ESPEC
  add constraint NN_DESPEC
  check ("DESPEC" IS NOT NULL);

