create table WCTEM_CUE_ESP_HAB_AUX
(
  DNI    VARCHAR2(9) not null,
  CUERPO VARCHAR2(4) not null,
  ESP    VARCHAR2(3) not null,
  DESP   VARCHAR2(80),
  IP     VARCHAR2(1)
)
;
comment on table WCTEM_CUE_ESP_HAB_AUX
  is 'Tabla auxiliar que contiene los cuepos y especialidades por las que puede concursar el participante.';
comment on column WCTEM_CUE_ESP_HAB_AUX.DNI
  is 'D.N.I.';
comment on column WCTEM_CUE_ESP_HAB_AUX.CUERPO
  is 'Cuerpo al que pertenece el participante.';
comment on column WCTEM_CUE_ESP_HAB_AUX.ESP
  is 'Especialidad.';
comment on column WCTEM_CUE_ESP_HAB_AUX.DESP
  is 'Descripción de la especialidad.';
comment on column WCTEM_CUE_ESP_HAB_AUX.IP
  is 'Indicador de Participación';
alter table WCTEM_CUE_ESP_HAB_AUX
  add constraint PK_WCTEM_CUE_ESP_HAB_AUX primary key (DNI, CUERPO, ESP);

