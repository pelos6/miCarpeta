create table WCTEM_PCENLOC
(
  PROV      VARCHAR2(2),
  ST        VARCHAR2(1),
  DNI       VARCHAR2(9),
  NORDEN    VARCHAR2(4) not null,
  CUERPO    VARCHAR2(4),
  ORDEN     NUMBER(3),
  CENLOC    VARCHAR2(9),
  DCENLOC   VARCHAR2(80),
  CESPEC    VARCHAR2(3),
  VERNA     VARCHAR2(1),
  FECULTMOD DATE not null,
  BILING    VARCHAR2(1) default '0' not null,
  ITIN      VARCHAR2(1)
)
;
comment on table WCTEM_PCENLOC
  is 'Tabla que contiene los datos de las peticiones de centro y/o localidad.';
comment on column WCTEM_PCENLOC.PROV
  is 'Ambito territorial de origen.';
comment on column WCTEM_PCENLOC.ST
  is 'Subdirección territorial.';
comment on column WCTEM_PCENLOC.DNI
  is 'D.N.I.';
comment on column WCTEM_PCENLOC.NORDEN
  is 'Numero de orden de la solicitud.';
comment on column WCTEM_PCENLOC.CUERPO
  is 'Cuerpo al que pertenece el participante.';
comment on column WCTEM_PCENLOC.ORDEN
  is 'Nº de orden de la petición del participante.';
comment on column WCTEM_PCENLOC.CENLOC
  is 'Código del centro o de la localidad que se solicita.';
comment on column WCTEM_PCENLOC.DCENLOC
  is 'Descripción del centro o de la localidad que se solicita.';
comment on column WCTEM_PCENLOC.CESPEC
  is 'Especialidad solicitada.';
comment on column WCTEM_PCENLOC.VERNA
  is 'Vernáculo de la plaza.';
comment on column WCTEM_PCENLOC.FECULTMOD
  is 'Fecha de última modificación del registro';
comment on column WCTEM_PCENLOC.BILING
  is 'Indicador de idioma para bilingüismo (1 - Inglés, 2 - Frances, 3 - Alemán)';
comment on column WCTEM_PCENLOC.ITIN
  is 'Indicador de itinerancia';
alter table WCTEM_PCENLOC
  add constraint PK_WCTEM_PCENLOC primary key (DNI, NORDEN, ORDEN);
alter table WCTEM_PCENLOC
  add constraint CK_BILING
  check (biling in ('0','1','2','3'));
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_CENLOC
  check ("CENLOC" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_CESPEC
  check ("CESPEC" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_CUERPO
  check ("CUERPO" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_DNI
  check ("DNI" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_ORDEN
  check ("ORDEN" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_PROV
  check ("PROV" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_ST
  check ("ST" IS NOT NULL);
alter table WCTEM_PCENLOC
  add constraint NN_PCENLOC_VERNA
  check ("VERNA" IS NOT NULL);

