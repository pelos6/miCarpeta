create table WCTEM_PFORPROV
(
  PROV      VARCHAR2(2),
  ST        VARCHAR2(1),
  DNI       VARCHAR2(9) not null,
  NORDEN    VARCHAR2(4) not null,
  CUERPO    VARCHAR2(4) not null,
  ORDEN     NUMBER(1) not null,
  PPROV     VARCHAR2(2) not null,
  PST       VARCHAR2(1),
  FECULTMOD DATE not null
)
;
comment on table WCTEM_PFORPROV
  is 'Tabla que contiene las peticiones de provincias para forzosos';
comment on column WCTEM_PFORPROV.PROV
  is 'Ambito territorial de origen.';
comment on column WCTEM_PFORPROV.ST
  is 'Subdirección territorial de origen.';
comment on column WCTEM_PFORPROV.DNI
  is 'D.N.I.';
comment on column WCTEM_PFORPROV.NORDEN
  is 'Numero de orden de la solicitud.';
comment on column WCTEM_PFORPROV.CUERPO
  is 'Cuerpo al que pertenece el participante.';
comment on column WCTEM_PFORPROV.ORDEN
  is 'Nº de orden de petición .';
comment on column WCTEM_PFORPROV.PPROV
  is 'Código de la provincia que se solicita.';
comment on column WCTEM_PFORPROV.PST
  is 'Código de la Subdirección territorial';
comment on column WCTEM_PFORPROV.FECULTMOD
  is 'Fecha de última modificación del registro';
alter table WCTEM_PFORPROV
  add constraint PK_WCTEM_PFORPROV primary key (DNI, NORDEN, CUERPO, ORDEN, PPROV);

