create table WCTEM_CENTROS
(
  CPROV       VARCHAR2(2),
  CCENTRO     VARCHAR2(8),
  COD_LOC     VARCHAR2(9),
  DCENTRO     VARCHAR2(50),
  CDENO_GENER VARCHAR2(3),
  TIPO_CENTRO VARCHAR2(1)
)
;
comment on table WCTEM_CENTROS
  is 'Tabla de Centros que se anuncian a concurso.';
comment on column WCTEM_CENTROS.CCENTRO
  is 'Código universal del Centro.';
comment on column WCTEM_CENTROS.COD_LOC
  is 'Código de la localidad del centro.';
comment on column WCTEM_CENTROS.DCENTRO
  is 'Denominación del Centro.';
comment on column WCTEM_CENTROS.CDENO_GENER
  is 'Denominación generica del Centro.';
comment on column WCTEM_CENTROS.TIPO_CENTRO
  is 'Tipo de Centro para el concurso.';
alter table WCTEM_CENTROS
  add constraint PK_WCTEM_CENTROS primary key (CCENTRO);
alter table WCTEM_CENTROS
  add constraint CK_TIPO_CENTRO
  check (Tipo_centro IN (NULL,'G','I','M','T','N'));
alter table WCTEM_CENTROS
  add constraint NN_CCENTRO
  check ("CCENTRO" IS NOT NULL);
alter table WCTEM_CENTROS
  add constraint NN_CDENO_GENER
  check ("CDENO_GENER" IS NOT NULL);
alter table WCTEM_CENTROS
  add constraint NN_COD_LOC
  check ("COD_LOC" IS NOT NULL);
alter table WCTEM_CENTROS
  add constraint NN_DCENTRO_ARA
  check ("DCENTRO" IS NOT NULL);

