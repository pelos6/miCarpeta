create table W_PAR
(
  DNI         VARCHAR2(15) not null,
  TIPO_ID     VARCHAR2(1) not null,
  F_NAC       DATE,
  SEX         VARCHAR2(1),
  NOM         VARCHAR2(22),
  APE1        VARCHAR2(22),
  APE2        VARCHAR2(22),
  COD_LOC_NAC VARCHAR2(9),
  COD_PAI_NAC VARCHAR2(3),
  DOM         VARCHAR2(50),
  COD_POS     VARCHAR2(5),
  COD_LOC_RES VARCHAR2(9),
  COD_PAI_RES VARCHAR2(3),
  TEL1        VARCHAR2(9),
  TEL2        VARCHAR2(9),
  F_ULT_MOD   DATE not null,
  CUE_COR     VARCHAR2(100),
  F_ULT_REP   DATE not null,
  L_PAS       VARCHAR2(1),
  COD_BAN     VARCHAR2(30)
)
;
comment on table W_PAR
  is 'Personas que están en cliem_participante o clipri_participante y tienen contraseña para proteger sus datos.';
comment on column W_PAR.DNI
  is ' DNI del participante ';
comment on column W_PAR.TIPO_ID
  is ' Tipo de identificador del participante ';
comment on column W_PAR.F_NAC
  is ' Fecha de nacimiento del participante ';
comment on column W_PAR.SEX
  is ' Sexo del participante ';
comment on column W_PAR.NOM
  is ' Nombre del participante ';
comment on column W_PAR.APE1
  is ' Primer apellido del participante ';
comment on column W_PAR.APE2
  is ' Segundo apellido del participante ';
comment on column W_PAR.COD_LOC_NAC
  is ' Código de localidad de nacimiento del participante ';
comment on column W_PAR.COD_PAI_NAC
  is ' Código de país de nacimiento del participante ';
comment on column W_PAR.DOM
  is ' Domicilio del participante ';
comment on column W_PAR.COD_POS
  is 'Codigo postal del participante ';
comment on column W_PAR.COD_LOC_RES
  is 'Codigo de localidad de residencia del participante ';
comment on column W_PAR.COD_PAI_RES
  is 'Codigo del pais de residencia del participante ';
comment on column W_PAR.TEL1
  is 'Primer telefono del participante ';
comment on column W_PAR.TEL2
  is 'Segundo telefono del participante ';
comment on column W_PAR.F_ULT_MOD
  is 'Fecha en que se ha realizado la última modificación a esta tabla ';
comment on column W_PAR.CUE_COR
  is 'Cuenta de correo electrónico';
comment on column W_PAR.F_ULT_REP
  is 'Fecha de última réplica en la que se han modificado datos personales o méritos del participante';
comment on column W_PAR.L_PAS
  is 'Si el participante tiene contraseña o no';
comment on column W_PAR.COD_BAN
  is 'Códigos bancarios para nómina IBAN a partir de febrero 2014';
alter table W_PAR
  add constraint PK_W_PAR primary key (DNI, TIPO_ID);

