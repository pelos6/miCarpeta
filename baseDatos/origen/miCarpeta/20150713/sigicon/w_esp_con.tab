create table W_ESP_CON
(
  COD_CON NUMBER(4) not null,
  COD_CUE VARCHAR2(4) not null,
  COD_ESP VARCHAR2(3) not null,
  COD_ACC VARCHAR2(1) not null,
  FAS_ESP VARCHAR2(2),
  COD_PRO VARCHAR2(2),
  TAS     NUMBER(5,2),
  MOD     VARCHAR2(3)
)
;
comment on table W_ESP_CON
  is ' Especialidades Convocadas en una Convocatoria especifica';
comment on column W_ESP_CON.COD_CON
  is ' Código de la convocatoria a la que corresponde ';
comment on column W_ESP_CON.COD_CUE
  is ' Código del cuerpo al que pertenece la especialidad ';
comment on column W_ESP_CON.COD_ESP
  is ' Código de la especialidad ';
comment on column W_ESP_CON.COD_ACC
  is ' Código del acceso de la especialidad (0 sin accesos)';
comment on column W_ESP_CON.FAS_ESP
  is 'Fase de la especialidad ';
comment on column W_ESP_CON.COD_PRO
  is 'Código de la provincia donde se realiza la prueba si la tiene.';
comment on column W_ESP_CON.TAS
  is 'Tasa a pagar por participar en el proceso ';
comment on column W_ESP_CON.MOD
  is 'Número del modelo de instancia que aparece en la solicitud';
alter table W_ESP_CON
  add constraint PK_W_ESP_CON primary key (COD_CON, COD_CUE, COD_ESP, COD_ACC);
alter table W_ESP_CON
  add constraint FK_ESP_CON_CON foreign key (COD_CON)
  references W_CON (COD_CON);

