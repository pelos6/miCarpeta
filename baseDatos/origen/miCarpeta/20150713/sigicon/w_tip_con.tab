create table W_TIP_CON
(
  COD_TIP_CON VARCHAR2(1) not null,
  DES_TIP_CON VARCHAR2(100)
)
;
comment on table W_TIP_CON
  is 'Tipos de convocatoria gestionadas por SIGICOM';
comment on column W_TIP_CON.COD_TIP_CON
  is 'Código del tipo de convocatoria ';
comment on column W_TIP_CON.DES_TIP_CON
  is 'Descripción del tipo de convocatoria';
alter table W_TIP_CON
  add constraint PK_W_TIP_CON primary key (COD_TIP_CON);

