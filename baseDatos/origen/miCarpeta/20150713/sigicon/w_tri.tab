create table W_TRI
(
  COD_CON NUMBER(4) not null,
  DNI     VARCHAR2(15) not null,
  TIPO_ID VARCHAR2(1) not null
)
;
comment on table W_TRI
  is 'Miembros del tribunal para una Convocatoria especifica';
comment on column W_TRI.COD_CON
  is ' Código de la convocatoria a la que corresponde ';
comment on column W_TRI.DNI
  is ' DNI del miembro del tribunal ';
comment on column W_TRI.TIPO_ID
  is ' Tipo de identificador del miembro del tribunal ';
alter table W_TRI
  add constraint PK_W_TRI primary key (COD_CON);
alter table W_TRI
  add constraint FK_TRI_CON foreign key (COD_CON)
  references W_CON (COD_CON);
alter table W_TRI
  add constraint FK_TRI_PAR foreign key (DNI, TIPO_ID)
  references W_PAR (DNI, TIPO_ID);

