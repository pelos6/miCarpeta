create table W_FAS_CON
(
  COD_FAS VARCHAR2(2) not null,
  DES_FAS VARCHAR2(100),
  L_ACT   VARCHAR2(1)
)
;
comment on table W_FAS_CON
  is 'Tabla con las distintas fases de una convocatoria ';
comment on column W_FAS_CON.COD_FAS
  is 'Código de la fase de una convocatoria ';
comment on column W_FAS_CON.DES_FAS
  is 'Descripción de la fase de una convocatoria ';
comment on column W_FAS_CON.L_ACT
  is 'Si la fase esta activa  ';
alter table W_FAS_CON
  add constraint PK_W_FAS_CON primary key (COD_FAS);

